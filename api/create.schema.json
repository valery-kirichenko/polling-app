{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "pattern": "^(\\s*\\S\\s*){1,}$"
    },
    "options": {
      "type": "array",
      "minItems": 2,
      "items": {
        "type": "string",
        "pattern": "^(\\s*\\S\\s*){1,}$"
      }
    },
    "custom_url": {
      "type": "string",
      "pattern": "^[0-9a-zA-Z-_]{3,64}$"
    }
  },
  "required": [
    "title",
    "options"
  ]
}